# Production Environment Variables for Networthy Backend
NODE_ENV=production
PORT=4000

# Database
PG_CONNECTION_STRING=your_production_supabase_connection_string

# Frontend URL (for CORS)
FRONTEND_URL=https://networthy.vercel.app

# OAuth Credentials - Replace with production keys
YOUTUBE_API_KEY=your_production_youtube_api_key
GOOGLE_CLIENT_ID=your_production_google_client_id
GOOGLE_CLIENT_SECRET=your_production_google_client_secret

TWITCH_CLIENT_ID=your_production_twitch_client_id
TWITCH_CLIENT_SECRET=your_production_twitch_client_secret

TIKTOK_CLIENT_KEY=your_production_tiktok_client_key
TIKTOK_CLIENT_SECRET=your_production_tiktok_client_secret

# Session Secret
SESSION_SECRET=your_production_session_secret

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=1000
